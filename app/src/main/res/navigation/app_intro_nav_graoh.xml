<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_intro_nav_graoh"
    app:startDestination="@id/selectLocationFragment">

    <activity
        android:id="@+id/appIntroActivity"
        android:name="com.janustech.helpsaap.ui.startup.AppIntroActivity"
        tools:layout="@layout/activity_app_intro" />

    <fragment
        android:id="@+id/selectLocationFragment"
        android:name="com.janustech.helpsaap.ui.startup.FragmentSelectLocationAndLanguage"
        tools:layout="@layout/fragment_select_location_and_language">
        <action
            android:id="@+id/action_selectLocationFragment_to_appIntroHome"
            app:destination="@id/appIntroHome">
        </action>

    </fragment>

    <fragment
        android:id="@+id/appIntroHome"
        android:name="com.janustech.helpsaap.ui.startup.AppIntroHomeFragment"
        tools:layout="@layout/fragment_app_intro_home">
        <action
            android:id="@+id/action_appIntroHome_to_appIntroSearchList"
            app:destination="@id/appIntroSearchList">
        </action>
        <action
            android:id="@+id/action_appIntroHome_to_selectLocationFragment"
            app:destination="@id/selectLocationFragment">
        </action>

    </fragment>

    <fragment
        android:id="@+id/appIntroSearchList"
        android:name="com.janustech.helpsaap.ui.startup.AppIntroSearchListFragment"
        tools:layout="@layout/fragment_app_intro_search_list">
        <action
            android:id="@+id/action_appIntroSearchList_to_selectLocationFragment"
            app:destination="@id/selectLocationFragment">
        </action>

    </fragment>


</navigation>